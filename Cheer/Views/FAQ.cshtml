@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "_Layout.cshtml";
}

@{ Html.CachedPartial("PageHeading", Model,
       Int32.Parse(System.Configuration.ConfigurationManager.AppSettings["cachedPartialExpiration"]),
       cacheByPage: true); }

@foreach (var item in Model.Content.Children)
{
    <div class="row">
        <div class="large-12 columns">
            <h3>
                <span class="gt">&gt;</span> @item.GetPropertyValue("QuestionHeading")
            </h3>

            @item.GetPropertyValue("Answer")
        </div>
    </div>
}